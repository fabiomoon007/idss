{
  "name": "idss-dashboard",
  "version": "1.0.0",
  "description": "IDSS Indicator Management Panel",
  "type": "module",
  "scripts": {
    "tsc": "tsc",
    "build:css": "tailwindcss -i ./src/input.css -o ./public/output.css",
    "copy:public": "copyfiles -u 1 \"public/**/*\" dist",
    "copy:fn-pkg": "copyfiles -u 2 \"netlify/functions/package.json\" \"dist/netlify/functions\"",
    "build": "npm run tsc && npm run build:css && npm run copy:public && npm run copy:fn-pkg",
    "start:dev": "python server.py"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "typescript": "^5.8.3",
    "lucide-react": "^0.400.0",
    "@google/genai": "^0.15.0",
    "recharts": "^2.12.7",
    "tailwindcss": "^3.4.3",
    "postcss": "^8.4.38",
    "autoprefixer": "^10.4.19",
    "copyfiles": "^2.4.1"
  }
}